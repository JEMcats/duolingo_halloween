[req]
default_bits = 2048
prompt = no
default_md = sha256
req_extensions = req_ext
distinguished_name = dn

[dn]
C = YourCountryCode (Example 'US')
ST = YourState (Only In U.S.A) (Example 'Maine')
L = YourCity (Example 'Portland')
O = YourName
OU = YourName
CN = localhost (Change If Needed)

[req_ext]
subjectAltName = @alt_names

[alt_names]
IP.1 = localhost (Change If Needed)